<package format="3">
    <name>aiim_rviz_plugins</name>
    <version>1.0.0</version>
    <description>Contains plugins to visualize object detection outputs</description>
    <maintainer email="info@aiim.ai">AIIM</maintainer>
    <license>NavInfo Europe B.V. The Netherlands - Confidential</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <build_depend>aiim_autoware_common</build_depend>
    <build_depend>qtbase5-dev</build_depend>

    <depend>aiim_autoware_msgs</depend>
    <depend>rviz_common</depend>
    <depend>rviz_default_plugins</depend>

    <exec_depend>libqt5-widgets</exec_depend>
    <exec_depend>rviz2</exec_depend>

    <test_depend>ament_lint_auto</test_depend>
    <test_depend>ament_lint_common</test_depend>

    <export><build_type>ament_cmake</build_type></export>
</package>
