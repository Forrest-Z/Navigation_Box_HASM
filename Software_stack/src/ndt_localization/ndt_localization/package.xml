<package format="2">
  <name>ndt_localization</name>
  <version>2.0.0</version>
  <description>The ndt_localization package</description>
  <maintainer email="info@aiim.ai">AIIM - Vehicles Team</maintainer>
  <license>NavInfo Europe B.V. The Netherlands - Confidential</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>aiim_ros_cmake</buildtool_depend>

  <exec_depend>aiim_rospy</exec_depend>

  <depend>aiim_autoware_msgs</depend>
  <!-- <depend>autoware_auto_vehicle_msgs</depend> -->
  <depend>ndt_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>launch</depend>
  <depend>launch_ros</depend>
  <depend>libpcl-all</depend>
  <depend>nav_msgs</depend>
  <depend>rclcpp</depend>
  <depend>sensor_msgs</depend>
  <depend>std_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
