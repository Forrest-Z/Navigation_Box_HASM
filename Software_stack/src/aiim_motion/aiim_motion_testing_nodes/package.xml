<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>aiim_motion_testing_nodes</name>
  <version>1.0.0</version>
  <description>
    A set of nodes to faciliate integration testing of motion controllers
  </description>
  <maintainer email="info@aiim.ai">AIIM</maintainer>
  <license>NavInfo Europe B.V. The Netherlands - Confidential</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <depend>aiim_autoware_msgs</depend>
  <depend>rclcpp</depend>
  <depend>tf2_msgs</depend>

  <build_depend>aiim_motion_common</build_depend>
  <build_depend>aiim_motion_testing</build_depend>
  <build_depend>aiim_time_utils</build_depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export><build_type>ament_cmake</build_type></export>
</package>
